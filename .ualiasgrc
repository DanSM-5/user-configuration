fif () {
  if [ ! "$#" -gt 0 ]; then
    echo 'Need a string to search for!'
    return 1;
  fi

  rg --color=always --files-with-matches --no-messages "$*" |
    fzf-defaults \
      --ansi --cycle \
      --preview "
        highlight -O ansi -l {} 2> /dev/null |
        rg --colors 'match:bg:yellow' --ignore-case --pretty --context 10 '$*' || rg --ignore-case --pretty --context 10 '$*' {}"
}

fdirs () {
